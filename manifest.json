{
  "name": "Qiita Hackathon 07",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This is an example to embed scripts to all web pages",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://qiita.com/*/items/*"
      ],
      "js": [
        "./js/lib/jquery-2.1.1.min.js",
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "add-fork-button.js"
  ],
  "background": {
    "scripts": [
      "add-fork-button.js"
    ]
  },
  "permissions": [
    "<all_urls>"
  ]
}
